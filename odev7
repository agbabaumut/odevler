select rating, count(*) from film
group by rating

"

""answer2
"

select replacement_cost , count(*) from film
group by replacement_cost
having count(*) > 50 

"

""answer3
"

select store_id, sum(customer_id) from customer
group by store_id

"

""answer4
"

select country_id,count(*) from city
group by country_id
order by count desc
limit 1


